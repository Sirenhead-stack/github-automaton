{
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.17.2",
    "common-tags": "^1.4.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.5.0",
    "express": "^4.15.3",
    "github": "^9.2.0",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "mention-bot": "^3.0.2",
    "moment": "^2.18.1",
    "nedb": "^1.8.0",
    "sync-request": "^4.1.0",
    "winston": "^2.3.1",
    "winston-color": "^1.0.0"
  },
  "scripts": {
    "clean": "rm -f bundle.zip && rm -rf dist",
    "bundle": "cd dist && zip -r ../bundle.zip . &&  cd - && zip -ru bundle.zip node_modules",
    "package": "npm run clean && npm run build && npm run bundle",
    "build": "node_modules/babel-cli/bin/babel.js src --out-dir dist --ignore tests",
    "postinstall": "npm run build",
    "run": "node dist/index.js"
  },
  "name": "github-bot",
  "version": "1.0.0",
  "description": "A repo management bot for github",
  "main": "src/index.js",
